P_0x55f174bd64c0 .param/l "WIDTH" 0 3 3, +C4<00000000000000000000000000001000>;
L_0x55f174c030c0 .functor BUFZ 8, v0x55f174c02750_0, C4<00000000>, C4<00000000>, C4<00000000>;
v0x55f174c02680_0 .net "clk", 0 0, v0x55f174c02b30_0;  alias, 1 drivers
v0x55f174c02750_0 .var "cnt", 7 0;
v0x55f174c02810_0 .net "en", 0 0, v0x55f174c02dd0_0;  alias, 1 drivers
v0x55f174c02910_0 .net "o_cnt", 7 0, L_0x55f174c030c0;  alias, 1 drivers
v0x55f174c029b0_0 .net "rst", 0 0, v0x55f174c02ec0_0;  alias, 1 drivers
    .scope S_0x55f174bebf40;
T_0 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x55f174c01f20_0, 0, 3;
    %end;
    .thread T_0;
    .scope S_0x55f174bebf40;
T_1 ;
    %wait E_0x55f174be9a20;
    %load/vec4 v0x55f174c02180_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x55f174c01f20_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55f174c02000_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x55f174c01f20_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x55f174c01f20_0, 0;
    %load/vec4 v0x55f174c01f20_0;
    %pad/u 32;
    %cmpi/e 7, 0, 32;
    %jmp/0xz  T_1.4, 4;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x55f174c01f20_0, 0;
T_1.4 ;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55f174c02310;
T_2 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55f174c02750_0, 0, 8;
    %end;
    .thread T_2;
    .scope S_0x55f174c02310;
T_3 ;
    %wait E_0x55f174be9a20;
    %load/vec4 v0x55f174c029b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x55f174c02750_0, 0;
    %jmp T_3.1;
T_3.0 ;
    %load/vec4 v0x55f174c02810_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.2, 8;
    %load/vec4 v0x55f174c02750_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x55f174c02750_0, 0;
    %load/vec4 v0x55f174c02750_0;
    %pad/u 32;
    %cmpi/e 249, 0, 32;
