S_0x55f174c02310 .scope module, "uut2" "counter_mod_n" 2 19, 3 1 0, S_0x55f174bda800;
 .timescale -12 -12;
    .port_info 0 /OUTPUT 8 "o_cnt";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "rst";
    .port_info 3 /INPUT 1 "en";
