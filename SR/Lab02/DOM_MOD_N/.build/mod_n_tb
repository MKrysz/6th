    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x55f174c02750_0, 0;
T_3.4 ;
T_3.2 ;
T_3.1 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55f174bda800;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f174c02b30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f174c02ec0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f174c02dd0_0, 0, 1;
    %end;
